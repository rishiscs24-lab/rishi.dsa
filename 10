#include <stdio.h>

#define SIZE 10

int ht[SIZE];

void insert(int key)
{
    int index = key % SIZE;

    while (ht[index] != -1)
        index = (index + 1) % SIZE;

    ht[index] = key;
}

void display()
{
    for (int i = 0; i < SIZE; i++)
        printf("%d : %d\n", i, ht[i]);
}

int main()
{
    int n, key;

    for (int i = 0; i < SIZE; i++)
        ht[i] = -1;

    printf("Enter number of employee records: ");
    scanf("%d", &n);

    printf("Enter %d keys:\n", n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &key);
        insert(key);
    }

    printf("Hash Table:\n");
    display();

    return 0;
}



o/p:-
Enter number of employee records: 1
Enter 1 keys:
1234
Hash Table:
0 : -1
1 : -1
2 : -1
3 : -1
4 : 1234
5 : -1
6 : -1
7 : -1
8 : -1
9 : -1
